{
    // Include everything
    "include": [
        "."
    ],
    "exclude": [
        "**/node_modules",
        "**/__pycache__",
        ".venv",
        ".vscode",
        ".github",
    ],
    "ignore": [
        "/.git",
    ],
    "defineConstant": {
        "DEBUG": true
    },
    "reportMissingImports": "error",
    "reportMissingTypeStubs": false,
    "reportAny": false,
    "reportExplicitAny": false,
}
